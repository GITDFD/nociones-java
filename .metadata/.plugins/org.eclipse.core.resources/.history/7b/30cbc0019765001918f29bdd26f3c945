package main;

//siempre que usemos una clae que no estoe en el paquete hay que importarla 
import entidades.Persona;

public class MainPOO {
	public static void main(String[] args) {
		Persona cliente = new Persona(); 				// el new de Persona hace que se cree el objeto.
		cliente.nombre = "Domingo Fernandez";
		cliente.edad = 45;
		cliente.peso = 95;
		
		Persona cliente2 = new Persona(); 				// el new de Persona hace que se cree el objeto.
		cliente2.nombre = "Guillermo Diaz";
		cliente2.edad = 40;
		cliente2.peso = 86;

	}
}
